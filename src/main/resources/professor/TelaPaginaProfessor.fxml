<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="proj.provas.aplicacao.view.controller.TelaPaginaProfessorController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="228.0">
         <image>
            <Image url="@/assets/PaginaDoProfessor.png" />
         </image>
      </ImageView>

      <Button fx:id="btnDadosProf" layoutX="300.0" layoutY="83.0" mnemonicParsing="false" onAction="#trocarParaDadosProf" prefHeight="36.0" prefWidth="245.0" text="Dados pessoais" />

      <Button fx:id="btnGerenciamento" layoutX="300.0" layoutY="143.0" mnemonicParsing="false" onAction="#trocarParaGerenciamento" prefHeight="36.0" prefWidth="245.0" text="Gerenciamento de disciplinas" />

      <Button fx:id="btnCadastroProvas" layoutX="300.0" layoutY="200.0" mnemonicParsing="false" onAction="#trocarParaListaProvas" prefHeight="36.0" prefWidth="245.0" text="Provas" />

      <Button fx:id="btnQuestoes" layoutX="300.0" layoutY="258.0" mnemonicParsing="false" onAction="#trocarParaQuestoes" prefHeight="36.0" prefWidth="245.0" text="Gerenciar Questões" />

      <Button layoutX="300.0" layoutY="316.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="245.0" text="Relatório de desempenho" />

      <Button layoutX="399.0" layoutY="360.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="48.0" text="Sair" />
   </children>
</AnchorPane>
